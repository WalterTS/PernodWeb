<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}PRM{% endblock %}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
        {% block stylesheets %}
            {% stylesheets
        'bundles/report/vendors/bootstrap/dist/css/bootstrap.min.css'
        'bundles/report/vendors/font-awesome/css/font-awesome.min.css'
        'bundles/report/vendors/nprogress/nprogress.css'
        'bundles/report/vendors/animate.css/animate.min.css'
        'bundles/report/build/css/custom.min.css'
        'bundles/report/css/style.css'
        filter='cssrewrite'
            %}
            <link rel="stylesheet" href="{{ asset_url }}" />
            {% endstylesheets %}
        {% endblock %}
    </head>
    <body class='{% block body_class %}{% endblock %}'>
        {% if app.request.hasPreviousSession %}
            {% for type, messages in app.session.flashbag.all() %}
                {% for message in messages %}
                    <div class="flash-{{ type }}">
                        {{ message }}
                    </div>
                {% endfor %}
            {% endfor %}
        {% endif %}
        <div>
            {% block fos_user_content %}
            {% endblock fos_user_content %}
        </div>
    </body>
</html>